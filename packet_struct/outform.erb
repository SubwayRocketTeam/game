/* pgen auto-generated packets */
#pragma once

struct PacketHeader{
  int size;
  int type;
};
#define PACKET(name) \
  struct name : PacketHeader{\
    name(){ \
      memset(this, 0, sizeof(name)); \
      type = PT_##name; \
      size = sizeof(name); \
    };
#define END };

enum packet_id{
  PT_PacketNone,
<%$_packets.each do |packet|%>
  PT_<%=packet.name%> = <%=packet.id%>,<%end%>

  PT_PacketMax
}

#pragma pack (push, 1)
<%$_packets.each do |packet|%>
<%=packet.dump%>
<%end%>
#pragma pack (pop)
